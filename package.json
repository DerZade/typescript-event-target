    "eslint-plugin-prettier": "^5.2.3",
    "jiti": "^2.4.2",
    "prettier": "^3.5.1",
    "tsd": "^0.24.1",
    "tsup": "^6.3.0",
    "typescript": "^4.9.5",
    "typescript-eslint": "^8.24.1"
  }
}
